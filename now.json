{
  "name": "my-first-deploy",
  "version": 2,
  "builds": [{"src": "server.js", "use":"@now/node"}],
  "routes": [{"src": "/(.*)", "dest": "/server.js"}],
  "env": {
    "DB_USER": "@mern-app-db-user",
    "DB_PASSWORD": "@mern-app-db-password",
    "DB_HOST": "@mern-app-db-host",
    "DB_NAME": "@mern-app-db-name"
  }
}